        "keyColor": "#ffdad7",
      //"outputColor": "#e7bdb9"
    },
    {
      "type": "gpu",
      "key": " ├ 󰢮  GPU ",
      "format": "{1} {2}",
      "keyColor": "#ffdad7",
      //"outputColor": "#e7bdb9"
    },
    {
        "type": "cpu",
        "key": " └   CPU ",
        "keyColor": "#ffdad7",
      //"outputColor": "#e7bdb9"
    },
    "break",
    "break",
    "colors"
  ]
