{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/master/doc/json_schema.json",
  "logo": {
    "type": "kitty",
    //"source": "/home/shorin/Pictures/picture.png",
    "width": 25,
    // "height":20,
    "padding": {
      "top": 1, // Top padding
      "left": 2, // Left padding
      "right": 2 // Right padding
    },
  },
  "display": {
    "separator": " ", // Separator between keys and values
    "color": {
      //"keys": "#e7bdb9", // Key color module名字的颜色
      "title": "#d8c2bf", // Title color 主机名的颜色
      "output": "#d8c2bf"
    },
  },
  "modules": [
    "break",
    {
      "type": "os", //这是哪个module
      "key": "OS", //module名字的显示
      // "keyColor": "#00ff00",      //module名字颜色
      // "format": "{name} {version}",    //具体内容
      "keyColor": "#ffb3ad",
   
    },
    {
      "type": "kernel",
      "key": " ├   KER ",
      "keyColor": "#ffb3ad",

    },
    {
      "type": "packages",
      "key": " ├   PAK ",
      "format": "{all}",
      "keyColor": "#ffb3ad",
    },
    {
      "type": "title",
      "key": " └   USR ",
      "keyColor": "#ffb3ad",
    },
    "break",
    "break",
    {
      "type": "wm",
      "key": "WM",
      "keyColor": "#e1c28c",
   },
    {
      "type": "de",
      "key": " ├ 󱈹  DES ",
      "keyColor": "#e1c28c",
     //"outputColor": "#e1c28c"
    },
    {
      "type": "shell",
      "key": " ├   SHE ",
      "keyColor": "#e1c28c",
      //"outputColor": "#e1c28c"
    },
    {
      "type": "terminal",
      "key": " ├   TER ",
      "keyColor": "#e1c28c",
      //"outputColor": "#e1c28c"
    },
    {
      "type": "terminalfont",
      "key": " └   TFO ",
      "keyColor": "#e1c28c",
      //"outputColor": "#e1c28c"
    },
    "break",
    "break",
    {
      "type": "host",
      "key": "PC ",
      "keyColor": "#ffdad7",
      //"outputColor": "#e7bdb9"
    },
    {
        "type": "memory",
        "key": " ├   MEM ",
        "keyColor": "#ffdad7",
      //"outputColor": "#e7bdb9"
    },
    {
